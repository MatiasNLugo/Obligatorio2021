<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    xmlns:tools="http://schemas.android.com/tools">


    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/frgListadoClientes"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="60"
        android:name="com.matiaslugo.obligatorio2021.viewclientes.ListadoClienteFragment"
        tools:layout="@layout/list_item_cliente" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/frmClienteDetalle"
        android:name="com.matiaslugo.obligatorio2021.viewclientes.DetalleClienteFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="40"
        tools:layout="@layout/fragment_cliente_detalle" />

</LinearLayout>
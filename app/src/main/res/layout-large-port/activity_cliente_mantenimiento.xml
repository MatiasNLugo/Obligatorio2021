        android:name="com.matiaslugo.obligatorio2021.viewclientes.ListadoClienteFragment"
        tools:layout="@layout/list_item_cliente"/>
    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/frmClienteDetalle"
